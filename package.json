{
  "name": "inquirer-ui",
  "version": "1.0.0",
  "description": "",
  "main": false,
  "private": true,
  "scripts": {
    "coveralls": "nyc report -r text-lcov | coveralls",
    "start": "babel-node make dev",
    "make": "babel-node make",
    "pretest": "xo",
    "test": "nyc ava -v",
    "posttest": "nyc report -r lcov",
    "watch": "chokidar '{src,test}/**/*.*' -c 'npm test'"
  },
  "repository": "shannonmoeller/inquirer-ui",
  "keywords": [],
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shannonmoeller/inquirer-ui/issues"
  },
  "homepage": "https://github.com/shannonmoeller/inquirer-ui",
  "dependencies": {
    "document-register-element": "^0.5.4",
    "dom-shims": "^0.1.4",
    "es5-shim": "^4.5.7",
    "es6-shim": "^0.35.0",
    "morphdom": "^1.1.2",
    "npm": "^3.8.2",
    "reset-css": "^2.0.2011012603",
    "svg4everybody": "^2.0.3",
    "whatwg-fetch": "0.11.0"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "browserify-incremental": "^3.1.0",
    "chalk": "^1.1.1",
    "chokidar-cli": "^1.2.0",
    "concat-stream": "^1.5.1",
    "cssnano": "^3.5.2",
    "errorify": "^0.3.1",
    "exorcist": "^0.4.0",
    "factor-bundle": "^2.5.0",
    "front-matter": "^2.0.6",
    "fs-extra": "^0.26.7",
    "fs-promise": "^0.5.0",
    "globby": "^4.0.0",
    "handlebars": "^4.0.5",
    "handlebars-layouts": "^3.1.3",
    "handlebars-wax": "^3.4.3",
    "jsdom": "^8.1.0",
    "nyc": "^6.1.1",
    "postcss": "^5.0.19",
    "postcss-apply": "^0.2.0",
    "postcss-cssnext": "^2.5.1",
    "postcss-import": "^8.0.2",
    "postcss-url": "5.1.1",
    "svgo": "^0.6.3",
    "svgstore": "^1.0.0",
    "xo": "^0.13.0",
    "ygor": "^1.5.4"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/helpers/browser.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "xo": {
    "env": [
      "browser",
      "node"
    ],
    "rules": {
      "brace-style": [2, "stroustrup"],
      "eqeqeq": [2, "allow-null"],
      "no-eq-null": 0,
      "object-curly-spacing": [2, "always"],
      "babel/object-curly-spacing": [2, "always"]
    }
  }
}
